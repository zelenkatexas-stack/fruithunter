--[[
    Blox Fruits AUTO-RESTART MYTHICAL HUNTER
    –ú–∏—Ñ–∏—á–µ—Å–∫–∏–µ —Ñ—Ä—É–∫—Ç—ã + —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø + Discord + –∞–≤—Ç–æ–ø–µ—Ä–µ–∑–∞–ø—É—Å–∫
--]]

getgenv().MythicalCollect = true

local Player = game:GetService("Players").LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")

-- –í–°–¢–ê–í–¨ –°–í–û–ô DISCORD WEBHOOK URL –°–Æ–î–ê
local WEBHOOK_URL = "https://discord.com/api/webhooks/your_webhook_url_here"

local function notify(msg)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "üêâ MYTHICAL HUNTER",
        Text = msg,
        Duration = 3
    })
end

-- –§—É–Ω–∫—Ü–∏—è –∞–≤—Ç–æ–ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–∞ –ø–æ—Å–ª–µ —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø–∞
local function SetupAutoRestart()
    -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —É—Å–ø–µ—à–Ω–æ–π —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
    TeleportService.TeleportInitiated:Connect(function()
        notify("üîÑ –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –Ω–∞—á–∞–ª–∞—Å—å, –≥–æ—Ç–æ–≤–ª—é –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫...")
        getgenv().MythicalCollect = false
    end)
    
    -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
    game:GetService("Players").LocalPlayer.OnTeleport:Connect(function(teleportState)
        if teleportState == Enum.TeleportState.InProgress then
            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–∫—Ä–∏–ø—Ç –¥–ª—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–≥–æ –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–∞
            queue_on_teleport([[
                getgenv().MythicalCollect = true
                loadstring(game:HttpGet("https://raw.githubusercontent.com/your_github/your_script.lua"))()
            ]])
        end
    end)
end

-- –û—Ç–ø—Ä–∞–≤–∫–∞ –≤ Discord
local function SendToDiscord(fruitName, serverId)
    if WEBHOOK_URL == "https://discord.com/api/webhooks/your_webhook_url_here" then
        return
    end
    
    local message = {
        ["content"] = "",
        ["embeds"] = {{
            ["title"] = "üêâ –ú–ò–§–ò–ß–ï–°–ö–ò–ô –§–†–£–ö–¢ –ù–ê–ô–î–ï–ù!",
            ["description"] = "**–§—Ä—É–∫—Ç:** " .. fruitName .. "\n**–°–µ—Ä–≤–µ—Ä:** " .. serverId,
            ["color"] = 0x00FF00,
            ["timestamp"] = DateTime.now():ToIsoDate(),
            ["footer"] = {
                ["text"] = "Blox Fruits Mythical Hunter"
            },
            ["thumbnail"] = {
                ["url"] = "https://cdn.discordapp.com/emojis/1093994460630167572.webp"
            }
        }}
    }
    
    local success = pcall(function()
        game:HttpGetAsync(WEBHOOK_URL, {
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = HttpService:JSONEncode(message)
        })
    end)
    
    if success then
        notify("üì® –û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ –≤ Discord!")
    end
end

-- –°–ø–∏—Å–æ–∫ –º–∏—Ñ–∏—á–µ—Å–∫–∏—Ö —Ñ—Ä—É–∫—Ç–æ–≤
local mythicalFruits = {
    "Dragon", "Leopard", "Mammoth", "T-Rex", "Dough",
    "Venom", "Control", "Spirit", "Shadow", "Blizzard",
    "Gravity", "Rumble"
}

-- –ë—ã—Å—Ç—Ä—ã–π –ø–æ–∏—Å–∫ –¢–û–õ–¨–ö–û –º–∏—Ñ–∏—á–µ—Å–∫–∏—Ö —Ñ—Ä—É–∫—Ç–æ–≤
local function QuickFindMythical()
    for _, obj in ipairs(workspace:GetChildren()) do
        if obj:IsA("Model") and obj:FindFirstChild("Handle") then
            for _, mythName in ipairs(mythicalFruits) do
                if string.find(obj.Name, mythName) then
                    notify("üêâ –ù–∞–π–¥–µ–Ω: " .. obj.Name)
                    return obj
                end
            end
        end
    end
    return nil
end

-- –ú–≥–Ω–æ–≤–µ–Ω–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è
local function InstantTeleport(fruit)
    if Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
        Player.Character.HumanoidRootPart.CFrame = fruit.Handle.CFrame
    end
end

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π –ø–æ–∏—Å–∫ –∏ –≤—ã–∑–æ–≤ RemoteEvent
local function FindAndCallRemote(fruit)
    local remoteNames = {
        "FruitCollect", "CollectFruit", "PickupFruit", 
        "GrabFruit", "CollectItem", "TakeFruit"
    }
    
    for _, name in ipairs(remoteNames) do
        local remote = ReplicatedStorage:FindFirstChild(name)
        if remote and remote:IsA("RemoteEvent") then
            for i = 1, 3 do
                remote:FireServer(fruit)
            end
            return true
        end
    end
    return false
end

-- –£–ª—É—á—à–µ–Ω–Ω—ã–π —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø —Å –∞–≤—Ç–æ–ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–æ–º
local function ServerHop()
    notify("üîÑ –ü–µ—Ä–µ—Ö–æ–¥ –Ω–∞ –¥—Ä—É–≥–æ–π —Å–µ—Ä–≤–µ—Ä...")
    
    local success, servers = pcall(function()
        local response = game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100")
        return HttpService:JSONDecode(response)
    end)
    
    if success and servers.data then
        for _, server in ipairs(servers.data) do
            if server.playing < server.maxPlayers and server.id ~= game.JobId then
                -- –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–∫—Ä–∏–ø—Ç –¥–ª—è –∞–≤—Ç–æ—Ä–µ—Å—Ç–∞—Ä—Ç–∞
                queue_on_teleport([[
                    getgenv().MythicalCollect = true
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/your_github/your_script.lua"))()
                ]])
                
                -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è
                TeleportService:TeleportToPlaceInstance(game.PlaceId, server.id)
                task.wait(5)
                return true
            end
        end
    end
    notify("‚ùå –ù–µ—Ç —Å–≤–æ–±–æ–¥–Ω—ã—Ö —Å–µ—Ä–≤–µ—Ä–æ–≤")
    return false
end

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –µ—Å—Ç—å –ª–∏ —Ñ—Ä—É–∫—Ç –≤ –∏–Ω–≤–µ–Ω—Ç–∞—Ä–µ
local function HasFruitInInventory(fruitName)
    local backpack = Player:FindFirstChild("Backpack")
    if backpack then
        for _, item in ipairs(backpack:GetChildren()) do
            if string.find(item.Name, fruitName) then
                return true
            end
        end
    end
    return false
end

-- –ì–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å –∞–≤—Ç–æ–ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–æ–º
local function Main()
    -- –ù–∞—Å—Ç—Ä–∞–∏–≤–∞–µ–º –∞–≤—Ç–æ—Ä–µ—Å—Ç–∞—Ä—Ç
    SetupAutoRestart()
    
    notify("üöÄ –ó–ê–ü–£–°–ö –û–•–û–¢–´ –ù–ê –ú–ò–§–ò–ß–ï–°–ö–ò–ï")
    notify("üîÑ –ê–≤—Ç–æ–ø–µ—Ä–µ–∑–∞–ø—É—Å–∫ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω")
    task.wait(2)

    while getgenv().MythicalCollect do
        local fruit = QuickFindMythical()
        
        if fruit then
            -- –ú–≥–Ω–æ–≤–µ–Ω–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è
            InstantTeleport(fruit)
            task.wait(0.5)
            
            -- –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ —á–µ—Ä–µ–∑ RemoteEvent
            if FindAndCallRemote(fruit) then
                notify("‚úÖ RemoteEvent —Å—Ä–∞–±–æ—Ç–∞–ª")
            end
            
            -- –ü—Ä—è–º–æ–µ –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ –≤ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—å
            local backpack = Player:FindFirstChild("Backpack")
            if backpack then
                local fruitClone = fruit:Clone()
                fruitClone.Parent = backpack
                notify("üíæ –§—Ä—É–∫—Ç –¥–æ–±–∞–≤–ª–µ–Ω –≤ —Ä—é–∫–∑–∞–∫")
            end
            
            -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∏ –æ—Ç–ø—Ä–∞–≤–∫–∞ –≤ Discord
            if HasFruitInInventory(fruit.Name) then
                notify("üéâ –£–°–ü–ï–•: " .. fruit.Name .. " –≤ –∏–Ω–≤–µ–Ω—Ç–∞—Ä–µ!")
                SendToDiscord(fruit.Name, game.JobId)
                task.wait(3)
            else
                notify("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å")
            end
            
        else
            notify("üêâ –ú–∏—Ñ–∏—á–µ—Å–∫–∏—Ö –Ω–µ—Ç - —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø")
            if ServerHop() then
                -- –ï—Å–ª–∏ —Å–µ—Ä–≤–µ—Ä—Ö–æ–ø —É—Å–ø–µ—à–µ–Ω, –≤—ã—Ö–æ–¥–∏–º –∏–∑ —Ü–∏–∫–ª–∞ (—Å–∫—Ä–∏–ø—Ç –ø–µ—Ä–µ–∑–∞–ø—É—Å—Ç–∏—Ç—Å—è)
                break
            else
                task.wait(10) -- –ñ–¥–µ–º –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–µ–π –ø–æ–ø—ã—Ç–∫–æ–π
            end
        end
        
        task.wait(2) -- –ü–∞—É–∑–∞ –º–µ–∂–¥—É –ø—Ä–æ–≤–µ—Ä–∫–∞–º–∏
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –æ—Å–Ω–æ–≤–Ω—É—é —Ñ—É–Ω–∫—Ü–∏—é
Main()

notify("‚úÖ –û–•–û–¢–ê –ó–ê–í–ï–†–®–ï–ù–ê")
